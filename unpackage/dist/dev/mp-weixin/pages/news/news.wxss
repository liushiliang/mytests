}
.img3-item .i3-h3 {
  font-size: 20rpx;
  line-height: 20rpx;
  color: #989898;
  padding-top: 20rpx;
  padding-bottom: 40rpx;
}
.black-wrapper {
  background: rgba(0, 0, 0, 0);
  width: 100%;
  height: 100%;
  z-index: 80;
  position: fixed;
  left: 0;
  top: 0;
  -webkit-transition: background 300ms ease-out;
  transition: background 300ms ease-out;
  -webkit-transition-delay: 1ms;
          transition-delay: 1ms;
}
.black-wrapper-show {
  background: rgba(0, 0, 0, 0.7);
}
.super-nav-block {
  background-color: #fff;
  position: fixed;
  z-index: 81;
  left: 0;
  top: -451rpx;
  width: 750rpx;
  padding-top: 42rpx;
  border-radius: 25rpx;
  -webkit-transition: all 300ms ease-out;
  transition: all 300ms ease-out;
  -webkit-transition-delay: 50ms;
          transition-delay: 50ms;
}
.super-nav-block .sn-h {
  top: 149rpx;
  line-height: 29rpx;
  font-size: 34rpx;
  color: #333;
  margin: 0 auto;
  width: 684rpx;
}
.super-nav-block .sn-btns {
  margin: 0 auto;
  margin-top: 42rpx;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  width: 684rpx;
  padding-bottom: 66rpx;
}
.super-nav-block .sn-btns .sn-item {
  margin-bottom: 28rpx;
  margin-right: 13.5rpx;
  width: 219rpx;
  height: 86rpx;
  line-height: 86rpx;
  text-align: center;
  font-size: 32rpx;
  letter-spacing: 4rpx;
  background-color: #f2f2f2;
  border-radius: 6rpx;
}
.super-nav-block .sn-btns .sn-item-selected {
  background-color: #1e95ee;
  color: #fff;
}
.super-nav-block .sn-btns .sn-item:nth-child(3n) {
  margin-right: 0;
}
.super-nav-block-show {
  top: 149rpx;
  -webkit-transition-delay: 0ms;
          transition-delay: 0ms;
}
.cancel_btn {
  width: 29rpx;
  height: 29rpx;
  position: absolute;
  right: 33rpx;
  top: 42rpx;
}

