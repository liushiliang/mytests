.me_bg {
  width: 750rpx;
}
.container {
  width: 663rpx;
  margin: 0 auto;
}
._self_page {
  position: absolute;
  top: 218rpx;
  width: 100%;
}
._self_page .user-info {
  margin-bottom: 55rpx;
}
._self_page .user-info .user-icon {
  width: 140rpx;
  height: 140rpx;
  border: 4rpx solid #fff;
  border-radius: 70rpx;
  margin-right: 31rpx;
  display: block;
  font-size: 0;
  line-height: 0;
}
._self_page .user-info .username {
  margin-top: 30rpx;
  line-height: 36rpx;
  font-size: 36rpx;
  color: #fff;
  padding-bottom: 27rpx;
}
._self_page .user-info .phone-number {
  font-size: 26rpx;
  color: #fff;
}
._self_page .me-lists {
  margin-top: 14rpx;
  border-radius: 25rpx;
  background-color: #fff;
}
._self_page .me-lists .mi {
  height: 116rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
._self_page .me-lists .mi > .fr-start {
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
._self_page .me-lists .mi .mi-icon {
  width: 72rpx;
}
._self_page .me-lists .mi .mi-h {
  color: #303233;
  font-size: 30rpx;
  line-height: 30rpx;
  width: 566rpx;
}
._self_page .me-lists .right-arrow {
  width: 13rpx;
  height: 21rpx;
}
._self_page image {
  display: block;
}
.centerline {
  width: 700rpx;
  height: 1rpx;
  background-color: #f0f0f0;
  margin-left: 118rpx;
}
.black-wrapper {
  display: none;
  -webkit-transition: all 200ms ease-out;
  transition: all 200ms ease-out;
  opacity: 0;
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.15);
  z-index: 200;
  top: 0;
  left: 0;
}
.show-wrapper {
  opacity: 1;
  display: block;
}
.info-block {
  width: 750rpx;
  position: fixed;
  bottom: -1000rpx;
  -webkit-transition: all 300ms ease-out;
  transition: all 300ms ease-out;
  -webkit-transition-delay: 200ms;
          transition-delay: 200ms;
  z-index: 201;
}
.info-block .container {
  margin: 0 auto;
  width: 672rpx;
}
.info-block .lists {
  border-radius: 5rpx;
}
.info-block .item {
  color: #898c91;
  font-size: 28rpx;
  background-color: #fff;
  height: 76rpx;
  line-height: 76rpx;
  letter-spacing: 4rpx;
}
.info-block .h {
  height: 79rpx;
  line-height: 79rpx;
  font-size: 26rpx;
  color: #a7aba7;
  letter-spacing: 2rpx;
}
.info-block .item-selected {
  color: #4698f4;
}
.info-block .footer {
  border-radius: 5rpx;
  margin-top: 28rpx;
  margin-bottom: 28rpx;
  width: 672rpx;
  margin-left: 39rpx;
}
.policy-block {
  width: 690rpx;
  height: 85%;
  border-radius: 20rpx;
  box-shadow: 0 0 20rpx #999;
  padding: 10rpx 20rpx;
  position: absolute;
  background-color: #fff;
  left: 30rpx;
  top: 5%;
  box-sizing: border-box;
}
.policy-block .pb-h {
  font-size: 34rpx;
  line-height: 80rpx;
  color: #333;
}
.policy-block .pb-h2 {
  font-size: 30rpx;
  line-height: 40rpx;
}
.policy-block .pb-content {
  font-size: 28rpx;
  line-height: 30rpx;
  color: #333;
}
.pb-btm {
  background-color: #1e95ee;
  width: 690rpx;
  font-size: 32rpx;
  position: absolute;
  top: 92%;
  color: #fff;
  left: 30rpx;
  border-radius: 20rpx;
  height: 6%;
  min-height: 38rpx;
  letter-spacing: 4rpx;
  box-shadow: 0 0 20rpx #999;
}
.black-wrapper2 {
  background-color: rgba(0, 0, 0, 0.2);
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 104;
}
.text-indent {
  margin-left: 56rpx;
}

